    "@types/pako": "^1.0.1",
    "@types/uuid": "^7.0.2",
    "buffer": "^5.5.0",
    "fast-deep-equal": "^3.1.1",
    "firebase": "^7.14.0",
    "hammerjs": "^2.0.8",
    "material-design-icons-iconfont": "^5.0.1",
    "ng2-logger": "3.0.11",
